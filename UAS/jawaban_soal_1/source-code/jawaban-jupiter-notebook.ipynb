    "def faktorial(n):\n",
    "    if n == 1:  # Base case\n",
    "        return 1\n",
    "    else:\n",
    "        return n * faktorial(n - 1)  # Recursive call\n",
    "\n",
    "# Pemanggilan recursive function\n",
    "hasil = faktorial(5)\n",
    "print(hasil)  # Output: 120"
   ]
  }
